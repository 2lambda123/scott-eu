
<launch>
  <node name="turtlebot2i_random_walker" pkg="base_robot" type="turtlebot2i_random_walker" output="screen">
    <!--<remap from="/turtlebot2i_random_walker" to="/vrep_ros_interface/turtlebot"/> -->
    <remap from="/turtlebot2i_random_walker/events/bumper"     to="/vrep_ros_interface/turtlebot/events/bumper"/>
    <remap from="/turtlebot2i_random_walker/events/cliff"      to="/vrep_ros_interface/turtlebot/events/cliff"/>
    <remap from="/turtlebot2i_random_walker/events/wheel_drop" to="/vrep_ros_interface/turtlebot/events/wheel_drop"/>
    <remap from="/turtlebot2i_random_walker/commands/velocity" to="/vrep_ros_interface/turtlebot/commands/velocity"/>
  </node>
</launch>
