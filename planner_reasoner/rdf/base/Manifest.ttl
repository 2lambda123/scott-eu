@prefix	lib: <http://www.swi-prolog.org/rdf/library/> .
@prefix	dcterms: <http://purl.org/dc/terms/> .
@prefix	pp: <http://ontology.cf.ericsson.net/planning_problem#> .
@prefix	wd: <http://ontology.cf.ericsson.net/warehouse_domain#> .
@prefix ppos: <http://ontology.cf.ericsson.net/planning_ontology_oslc_shapes#> .

<ppos>
		a lib:Schema ;
		a lib:Virtual ;
		dcterms:title "Vocabulary of OSLC shapes for PDDL planning" ;
		lib:source ppos: ;
		lib:schema <pp_oslc_shapes.ttl> .

<pp>
	a lib:Schema ;
	a lib:Virtual ;
	dcterms:title "Vocabulary of class relations between OSLC shapes of PDDL" ;
	lib:source pp: ;
	lib:schema <pp.ttl> .

<wd>
	a lib:Instances ;
	a lib:Virtual ;
	dcterms:title "Sample planning PDDL domain" ;
	lib:source wd: ;
	lib:schema <warehouse_domain.ttl> .
